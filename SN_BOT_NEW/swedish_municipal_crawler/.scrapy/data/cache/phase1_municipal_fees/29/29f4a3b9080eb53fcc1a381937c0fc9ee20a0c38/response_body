    <script src="/dist/js/jsbundle.min.js?cachebust=Release_6773"></script>



    <script>
        require(["uppsalainit", "datepickerwrapper", "matomowrapper", "formpage", "jquery", "toggleswitch"],
            function (uppsalainit, datepickerwrapper, matomowrapper, formpage, $, toggleSwitch) {
            uppsalainit.init();
            formpage.init();
            datepickerwrapper.init();


            
            matomowrapper.init({
                matomoTagManagerContainerId: "3JY0lyAG",
                matomoSiteId: "3"
            });
            

            $.lazyLoadXT.onload.addClass = 'animated fadeIn';

            // Toggle switch logic for switching between content areas within each block
            $(".js-toggle-element-two").on("click", function () {
                var $currentBlock = $(this).closest(".toggleblock-wrapper"); // Scope to the closest block container
                var isActive = $(this).hasClass("active"); // Check if the clicked button is already active

                // If the clicked button is not active, proceed with toggling
                if (!isActive) {
                    // Update active state for the buttons within the current block
                    $currentBlock.find(".js-toggle-element-two").removeClass("active").attr("aria-selected", "false");
                    $(this).addClass("active").attr("aria-selected", "true");

                    // Show or hide the content areas based on the button clicked
                    if ($(this).attr("id") === "section1-tab") {
                        // Show section 1, hide section 2
                        $currentBlock.find(".contentarea-section1").show();
                        $currentBlock.find(".contentarea-section2").hide();
                    } else {
                        // Show section 2, hide section 1
                        $currentBlock.find(".contentarea-section1").hide();
                        $currentBlock.find(".contentarea-section2").show();
                    }
                }
            });

            // Initialize toggle switch for each block's buttons
            $(".toggle-switch-container-fullwidth").each(function () {
                var $container = $(this); // The current container for the toggle switch
                var switchButton = $container.find(".toggle-switch-fullwidth"); // Find the toggle switch element
                var buttonId = switchButton.attr("id"); // Get the ID of the toggle switch
                var switchPointer = $container.find(".toggle-switch--pointer"); // Find the pointer element
                var pointerId = switchPointer.attr("id"); // Get the ID of the pointer

                // Initialize the toggle switch functionality using the found IDs
                toggleSwitch.initButtonToggle(
                    ".js-toggle-element-two", // Selector for the toggle buttons
                    ".active.js-toggle-element-two", // Selector for the active toggle button
                    "#" + pointerId, // ID of the pointer element
                    "#" + buttonId // ID of the toggle switch element
                );
            });
        });
    </script>


    
    

