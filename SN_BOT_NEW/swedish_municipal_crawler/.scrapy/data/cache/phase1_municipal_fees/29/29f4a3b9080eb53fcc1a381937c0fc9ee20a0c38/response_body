    <script>

        require(["urlservice", "opengraphhandler"], function (urlService, openGraphHandler) {

            var params = {
                tags: [
                    { tagType: "property", selector: "og:url", content: window.location }
                ]
            };

            urlService.initUrlChangeListener(openGraphHandler.updateMetaTags, params);
        });
    </script>

